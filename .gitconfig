[user]
	name = Mirko Rovere
	email = 04mirko@libero.it
[core]
	editor = vim
[init]
	defaultBranch = main
[color]
	ui = true
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[alias]
	co = commit
	com = commit -m
	log = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
